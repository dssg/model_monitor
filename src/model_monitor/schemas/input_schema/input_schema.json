{
    "model_groups": [
        {
            "name": "model_group_id",
            "dtype": "Integer",
            "description": "model group ID, unique identifier",
            "nullable": "false",
            "constraints": "PK"
        },
        {
            "name": "model_type",
            "dtype": "Text",
            "description": "model class definition, must be importable in python",
            "nullable": "false",
            "constraints": ""
        },
        {
            "name": "model_parameters",
            "dtype": "JSONB",
            "description": "model parameters passed to model_type, by keyword",
            "nullable": "true",
            "constraints": ""
        },
        {
            "name": "feature_list",
            "dtype": "ARRAY(Text)",
            "description": "text list of feature names, comma delimited",
            "nullable": "false",
            "constraints": ""
        }
    ],
    "models": [
        {
            "name": "model_id",
            "dtype": "Integer",
            "description": "model ID, unique identifier",
            "nullable": "false",
            "constraints": "PK"
        },
        {
            "name": "model_group_id",
            "dtype": "Integer",
            "description": "model group ID",
            "nullable": "false",
            "constraints": "FK"
        },
        {
            "name": "as_of_date",
            "dtype": "DateTime",
            "description": "effective end of training time",
            "nullable": "false",
            "constraints": ""
        },
        {
            "name": "train_matrix_id",
            "dtype": "Text",
            "description": "training matrix unique ID",
            "nullable": "false",
            "constraints": ""
        }
    ],
    "predictions": [
        {
            "name": "model_id",
            "dtype": "Integer",
            "description": "model ID",
            "nullable": "false",
            "constraints": "PK, FK"
        },
        {
            "name": "test_matrix_id",
            "dtype": "Text",
            "description": "testing matrix unique ID",
            "nullable": "false",
            "constraints": "PK"
        },
        {
            "name": "entity_id",
            "dtype": "Integer",
            "description": "entity unique ID",
            "nullable": "false",
            "constraints": "PK"
        },
        {
            "name": "score",
            "dtype": "Float",
            "description": "predicted score",
            "nullable": "false",
            "constraints": ""
        },
        {
            "name": "label",
            "dtype": "Integer",
            "description": "true outcome",
            "nullable": "false",
            "constraints": ""
        }
    ],
    "feature_importances": [
        {
            "name": "model_id",
            "dtype": "Integer",
            "description": "model ID",
            "nullable": "false",
            "constraints": "PK, FK"
        },
        {
            "name": "feature",
            "dtype": "Text",
            "description": "feature name, unique ID",
            "nullable": "false",
            "constraints": "PK"
        },
        {
            "name": "feature_importance",
            "dtype": "Float",
            "description": "estimated feature contribution",
            "nullable": "false",
            "constraints": ""
        }
    ]
}
