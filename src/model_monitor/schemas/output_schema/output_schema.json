{
    "distribution_metadata": [
        {
            "name": "distribution_metadata_id",
            "dtype": "Integer",
            "description": "distribution metadata unique ID",
            "nullable": "false",
            "constraints": "PK"
        },
        {
            "name": "is_discrete",
            "dtype": "Boolean",
            "description": "if True, assumes support is quantized and tracks all unique samples",
            "nullable": "false",
            "constraints": ""
        },
        {
            "name": "is_nullable",
            "dtype": "Boolean",
            "description": "if non-nullable, null values are removed",
            "nullable": "true",
            "constraints": ""
        },
        {
            "name": "default_type",
            "dtype": "Text",
            "description": "default ``numpy`` type to cast samples",
            "nullable": "true",
            "constraints": ""
        },
        {
            "name": "default_value",
            "dtype": "Float",
            "description": "default value for imputation purposes",
            "nullable": "true",
            "constraints": ""
        },
        {
            "name": "use_default_value_on_unsafe_cast",
            "dtype": "Boolean",
            "description": "if cast fails, use imputed value defined above",
            "nullable": "true",
            "constraints": ""
        },
        {
            "name": "support_minimum",
            "dtype": "Float",
            "description": "minimum sample value",
            "nullable": "true",
            "constraints": ""
        },
        {
            "name": "support_maximum",
            "dtype": "Float",
            "description": "maximum support value",
            "nullable": "true",
            "constraints": ""
        },
        {
            "name": "remove_samples_out_of_support",
            "dtype": "Boolean",
            "description": "if True, removes values that violate support bounds",
            "nullable": "true",
            "constraints": ""
        },
        {
            "name": "is_online",
            "dtype": "Boolean",
            "description": "if True, distribution supports multiple sample updates",
            "nullable": "true",
            "constraints": ""
        },
        {
            "name": "warn_on_online_support_change",
            "dtype": "Boolean",
            "description": "if online and support changes after update, raise warning",
            "nullable": "true",
            "constraints": ""
        },
        {
            "name": "interpolation_mode",
            "dtype": "Text",
            "description": "if using CDF point estimate interpolation, specify method",
            "nullable": "true",
            "constraints": ""
        },
        {
            "name": "tracking_mode",
            "dtype": "Text",
            "description": "quantization method for continuous distributions",
            "nullable": "true",
            "constraints": ""
        },
        {
            "name": "n_quantiles",
            "dtype": "Integer",
            "description": "number of evenly spaced quantiles",
            "nullable": "true",
            "constraints": ""
        },
        {
            "name": "n_lower_tail_quantiles",
            "dtype": "Integer",
            "description": "number of lower tail quantiles, in powers of ten",
            "nullable": "true",
            "constraints": ""
        },
        {
            "name": "n_upper_tail_quantiles",
            "dtype": "Integer",
            "description": "number of upper tail quantiles, in powers of ten",
            "nullable": "true",
            "constraints": ""
        },
        {
            "name": "custom_quantiles",
            "dtype": "ARRAY(Float)",
            "description": "list of custom quantile values (all in [0, 1])",
            "nullable": "true",
            "constraints": ""
        },
        {
            "name": "histogram_min",
            "dtype": "Float",
            "description": "minimum histogram bin value",
            "nullable": "true",
            "constraints": ""
        },
        {
            "name": "histogram_max",
            "dtype": "Float",
            "description": "maximum histogram bin value",
            "nullable": "true",
            "constraints": ""
        },
        {
            "name": "n_histogram_bins",
            "dtype": "Integer",
            "description": "number of evenly spaced histogram bins",
            "nullable": "true",
            "constraints": ""
        },
        {
            "name": "custom_histogram_bins",
            "dtype": "Text",
            "description": "list of custom histogram bins",
            "nullable": "true",
            "constraints": ""
        },
        {
            "name": "n_clusters",
            "dtype": "Integer",
            "description": "number of clusters to quantize",
            "nullable": "true",
            "constraints": ""
        },
        {
            "name": "clustering_algorithm",
            "dtype": "Text",
            "description": "clustering algorithm name (as specified by ``sklearn.cluster``)",
            "nullable": "true",
            "constraints": ""
        },
        {
            "name": "clustering_algorithm_kwargs",
            "dtype": "JSONB",
            "description": "arguments to pass to the algorithm class constructor",
            "nullable": "true",
            "constraints": ""
        },
        {
            "name": "clustering_parametric_family",
            "dtype": "Text",
            "description": "if specified, fits parametric estimate to each cluster, else does interpolation",
            "nullable": "true",
            "constraints": ""
        },
        {
            "name": "parametric_family",
            "dtype": "Text",
            "description": "name of parametric family (as specified by ``scipy.stats``)",
            "nullable": "true",
            "constraints": ""
        }
    ],
    "random_variables": [
        {
            "name": "rv_id",
            "dtype": "Integer",
            "description": "random variable ID",
            "nullable": "false",
            "constraints": "PK"
        },
        {
            "name": "rv_name",
            "dtype": "Text",
            "description": "random variable name",
            "nullable": "false",
            "constraints": "unique"
        },
        {
            "name": "rv_type",
            "dtype": "Text",
            "description": "random variable type",
            "nullable": "false",
            "constraints": ""
        },
        {
            "name": "distribution_metadata_id",
            "dtype": "Integer",
            "description": "distribution_metadata_id",
            "nullable": "true",
            "constraints": ""
        },
        {
            "name": "source_table",
            "dtype": "Text",
            "description": "feature source table",
            "nullable": "true",
            "constraints": ""
        },
        {
            "name": "is_test",
            "dtype": "Boolean",
            "description": "if True, use test-associated data",
            "nullable": "true",
            "constraints": ""
        },
        {
            "name": "latent_variable_name",
            "dtype": "Integer",
            "description": "observed feature (independent of calculation type)",
            "nullable": "false",
            "constraints": ""
        },
        {
            "name": "agg_func",
            "dtype": "Text",
            "description": "aggregation function used to generate feature",
            "nullable": "false",
            "constraints": ""
        },
        {
            "name": "time_agg",
            "dtype": "Text",
            "description": "time interval (in postgresql interval format)",
            "nullable": "false",
            "constraints": ""
        }
    ],
    "quantiles": [
        {
            "name": "rv_id",
            "dtype": "Integer",
            "description": "random variable ID",
            "nullable": "false",
            "constraints": "PK, FK"
        },
        {
            "name": "model_id",
            "dtype": "Integer",
            "description": "model ID",
            "nullable": "false",
            "constraints": "PK, ~FK"
        },
        {
            "name": "quantile",
            "dtype": "Float",
            "description": "quantile, defined from [0, 1] inclusive",
            "nullable": "false",
            "constraints": "PK"
        },
        {
            "name": "quantile_value",
            "dtype": "Float",
            "description": "quantile value",
            "nullable": "true",
            "constraints": ""
        }
    ],
    "histograms": [
        {
