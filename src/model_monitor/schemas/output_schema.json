{
    "distribution_metadata": [
        {
            "name": "distribution_metadata_id",
            "dtype": "int",
            "description": "distribution metadata unique identifier",
            "nullable": "false",
            "constraints": "PK"
        },
        {
            "name": "default_type",
            "dtype": "text",
            "description": "default Python (numpy) data type",
            "nullable": "true",
            "constraints": ""
        },
        {
            "name": "default_value",
            "dtype": "text",
            "description": "default distribution value",
            "nullable": "true",
            "constraints": ""
        },
        {
            "name": "use_default_value_on_unsafe_cast",
            "dtype": "bool",
            "description": "if true, replace failed casts with default value",
            "nullable": "true",
            "constraints": ""
        },
        {
            "name": "is_online",
            "dtype": "bool",
            "description": "indicator for online distribution",
            "nullable": "true",
            "constraints": ""
        },
        {
            "name": "warn_on_online_support_change",
            "dtype": "bool",
            "description": "if true and support changes between online updates, raise warning",
            "nullable": "true",
            "constraints": ""
        },
        {
            "name": "is_discrete",
            "dtype": "bool",
            "description": "indicator for discrete distribution",
            "nullable": "false",
            "constraints": ""
        },
        {
            "name": "support_maximum",
            "dtype": "float",
            "description": "support maximum value",
            "nullable": "true",
            "constraints": ""
        },
        {
            "name": "support_minimum",
            "dtype": "float",
            "description": "support minimum value",
            "nullable": "true",
            "constraints": ""
        },
        {
            "name": "remove_samples_out_of_support",
            "dtype": "bool",
            "description": "if true, exclude values from unspecified support",
            "nullable": "true",
            "constraints": ""
        },
        {
            "name": "is_nullable",
            "dtype": "bool",
            "description": "column nullability",
            "nullable": "false",
            "constraints": ""
        },
        {
            "name": "tracking_mode",
            "dtype": "text",
            "description": "if true, dynamically track quantile estimates for distribution",
            "nullable": "false",
            "constraints": ""
        },
        {
            "name": "n_quantiles",
            "dtype": "int",
            "description": "number of evenly-spaced quantiles to specify ",
            "nullable": "true",
            "constraints": ""
        },
        {
            "name": "n_lower_tail_quantiles",
            "dtype": "int",
            "description": "number of lower tail quantiles to include, up to 10^-n",
            "nullable": "true",
            "constraints": ""
        },
        {
            "name": "n_upper_tail_quantiles",
            "dtype": "int",
            "description": "number of upper tail quantiles to include, up to 1 - 10^-n",
            "nullable": "true",
            "constraints": ""
        },
        {
            "name": "custom_quantiles",
            "dtype": "text",
            "description": "comma-delimited list of custom quantiles",
            "nullable": "true",
            "constraints": ""
        },
        {
            "name": "histogram_min",
            "dtype": "float",
            "description": "smallest bin value",
            "nullable": "true",
            "constraints": ""
        },
        {
            "name": "histogram_max",
            "dtype": "float",
            "description": "largest bin value",
            "nullable": "true",
            "constraints": ""
        },
        {
            "name": "left_inclusive",
            "dtype": "bool",
            "description": "if true, bins are inclusive of their left values",
            "nullable": "true",
            "constraints": ""
        },
        {
            "name": "count_outside_range",
            "dtype": "bool",
            "description": "if true, all values outside range are counted, else classified as null",
            "nullable": "true",
            "constraints": ""
        },
        {
            "name": "n_histogram_bins",
            "dtype": "int",
            "description": "number of evenly spaced histogram bins to use",
            "nullable": "true",
            "constraints": ""
        },
        {
            "name": "custom_histogram_bins",
            "dtype": "text",
            "description": "comma-delimited list of custom histogram bins",
            "nullable": "true",
            "constraints": ""
        },
        {
            "name": "n_clusters",
            "dtype": "int",
            "description": "number of clusters to characterize; if null, inferred from distribution",
            "nullable": "true",
            "constraints": ""
        },
        {
            "name": "clustering_algorithm",
            "dtype": "text",
            "description": "clustering algorithm name",
            "nullable": "true",
            "constraints": ""
        },
        {
            "name": "clustering_algorithm_kwargs",
            "dtype": "jsonb",
            "description": "clustering algorithm kwargs",
            "nullable": "true",
            "constraints": ""
        },
        {
            "name": "parametric_family",
            "dtype": "text",
            "description": "parametric family name",
            "nullable": "true",
            "constraints": ""
        }
    ],
    "random_variables": [
        {
            "name": "rv_id",
            "dtype": "int",
            "description": "random variable ID, unique indentifier",
            "nullable": "false",
            "constraints": "PK"
        },
        {
            "name": "rv_name",
            "dtype": "text",
            "description": "random variable name",
            "nullable": "false",
            "constraints": "unique"
        },
        {
            "name": "rv_type",
            "dtype": "text",
            "description": "random variable type",
            "nullable": "false",
            "constraints": ""
        },
        {
            "name": "distribution_metadata_id",
            "dtype": "int",
            "description": "distribution_metadata_id",
            "nullable": "true",
            "constraints": ""
        },
        {
            "name": "source_table",
            "dtype": "text",
            "description": "feature source table",
            "nullable": "true",
            "constraints": ""
        },
        {
            "name": "latent_variable_name",
            "dtype": "int",
            "description": "observed feature (independent of calculation type)",
            "nullable": "false",
            "constraints": ""
        },
        {
            "name": "agg_func",
            "dtype": "text",
            "description": "aggregation function used to generate feature",
            "nullable": "false",
            "constraints": ""
        },
        {
            "name": "time_agg",
            "dtype": "text",
            "description": "time interval (in postgresql interval format)",
            "nullable": "false",
            "constraints": ""
        }
    ],
    "moments": [
        {
            "name": "rv_id",
            "dtype": "int",
            "description": "random variable ID",
            "nullable": "false",
            "constraints": "PK, FK"
        },
        {
            "name": "model_id",
            "dtype": "int",
            "description": "model ID",
            "nullable": "false",
            "constraints": "PK, ~FK"
        },
        {
            "name": "as_of_date",
            "dtype": "timestamp",
