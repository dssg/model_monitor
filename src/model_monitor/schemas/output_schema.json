            "nullable": "false",
            "constraints": "PK"
        },
        {
            "name": "moment_index",
            "dtype": "int",
            "description": "index representing ith moment",
            "nullable": "false",
            "constraints": "PK"
        },
        {
            "name": "moment_value",
            "dtype": "float",
            "description": "moment value",
            "nullable": "true",
            "constraints": ""
        }
    ],
